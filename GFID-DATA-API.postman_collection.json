	"item": [
		{
			"name": "auth",
			"item": [
				{
					"name": "hello cleint authentication",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const result = pm.response.json();\r",
									"//pm.globals.set(\"access_token\", result.access_token);\r",
									"const token = result.access_token;\r",
									"pm.environment.set(\"auth_token\", token);"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									""
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "urlencoded",
							"urlencoded": [
								{
									"key": "client_id",
									"value": "{{CLIENT_ID}}",
									"type": "text"
								},
								{
									"key": "client_secret",
									"value": "{{CLIENT_SECRET}}",
									"type": "text"
								},
								{
									"key": "grant_type",
									"value": "client_credentials",
									"type": "text"
								}
							]
						},
						"url": "{{SERVER_URL}}/realms/{{REALM}}/protocol/openid-connect/token"
					},
					"response": []
				}
			]
		},
		{
			"name": "etl-data",
